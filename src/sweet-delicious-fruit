<script type="text/javascript">
	$(document).ready(function(){ // Advanced search options
		
		$.fn.select_to_input = function(opt, formselector) {
			var f = $(this);
			var val = $("option:selected", $(this)).attr('value');
			var sf = $(formselector);
			var inp = $("input[name='"+opt+"']", sf);
			if (val == '') {
				inp.remove();
				return false;
			}
			if (!inp.length) {
				sf.append('<input type="hidden" name="'+opt+'" value="'+val+'">');
			} else {
				inp.attr('value', val);
			}			
		};

		$.fn.input_to_input = function(opt, formselector) {
			var f = $(this);
			var val = $(this).attr('value');
			var sf = $(formselector);
			var inp = $("input[name='"+opt+"']", sf);
			if (val == '') {
				inp.remove();
				return false;
			}
			if (!inp.length) {
				sf.append('<input type="hidden" name="'+opt+'" value="'+val+'">');
			} else {
				inp.attr('value', val);
			}			
		};

		$.fn.id_to_input = function(opt, formselector) {
			var f = $(this);
			var val = $(this).attr('id');
			var sf = $(formselector);
			var inp = $("input[name='"+opt+"']", sf);
			if (val == '') {
				inp.remove();
				return false;
			}
			if (!inp.length) {
				sf.append('<input type="hidden" name="'+opt+'" value="'+val+'">');
			} else {
				inp.attr('value', val);
			}			
		};		

		$("#sc-block > .sc > a").click(function() {
			
			if (!$(this).hasClass('active')) {
				$(this).id_to_input('co', '#searchform > .box');
				$("#sc-block > .sc > a.active").removeClass('active');
				$(this).addClass('active');
			} else {
				$(this).removeClass('active');
				$("#searchform input[name='co']").remove();
			}
		});
		
		$("#as-date").change(function() {
			$(this).select_to_input('dt', '#searchform > .box');
		});
		$("#as-shape").change(function() {
			$(this).select_to_input('or', '#searchform > .box');
		});
		
		$("#as-category").change(function() {
			$(this).select_to_input('ci', '#searchform > .box');		
		});
		$("#as-photographer").change(function() {
			$(this).input_to_input('pg', '#searchform > .box');		
		});		
		
	});
</script>
<script>
        $(document).ready(function() {
            $('#searchform').submit(function(e) {
                  // e.preventDefault();
         var searchString = $('input.input_box', this).val().replace(/ /g, '+')
			$(this).attr('action', '/free-photos/' + searchString)
             //window.location.href = '/free-photos/' + searchString;
			//e.preventDefault()
            });
         });
</script>
