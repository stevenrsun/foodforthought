<script type="text/javascript">
	$(function(){
		$('.login').click(function(e){
			e.preventDefault();
			var rr =  '//'+window.location.host + window.location.pathname + window.location.search;
			rr = encodeURIComponent(rr);
			var pathArray = window.location.pathname.split( '/' );
			if(pathArray[1]!='login')
			{
				var returnurl = '?returnurl='+rr;
			}else{
				var returnurl = '';
			}
			window.location.href=$(this).attr('href')+returnurl;
		});

		$('#page-menu-wrap').find('form').attr('id','smpl_search');

		$('#smpl_search').one('submit', function(e) {
		    e.preventDefault();
		    var elm = $(this).find('input[name="query"]');
		    var str = elm.val();
			var tt = str.replace(/[^a-z0-9\s]/gi, ' ').replace(/[_\s]/g, ' ');
			tt = tt.split(' ');
			tt = tt.filter(function(v){return v!==''});
			tt = tt.join(' ');	
			elm.val(tt);
		    // and when you done:
		    $(this).submit();
		});

		$('#searchform').one('submit', function(e) {
		    e.preventDefault();
		    var str = $('#search_input').val();
			var tt = str.replace(/[^a-z0-9\s]/gi, ' ').replace(/[_\s]/g, ' ');
			tt = tt.split(' ');
			tt = tt.filter(function(v){return v!==''});
			tt = tt.join(' ');	
			$('#search_input').val(tt);
		    // and when you done:
		    $(this).submit();
		});
	});
</script>
</body>
</html><script type="text/javascript">

	var tmpar = [];
	function searchcolletion(start,val,perpage)
	{
	  var tt = [];
	  $('.pg').html('');

	  var searchField = val;
	  var expression = new RegExp(searchField, "i");

	  $.when( 
	    $.each(tmpar, function(key, value){
	      if (value.search(expression) != -1)
	      {
	        tt.push(value);
	      }
	    }) 
	  ).done(function() {
	  	$('.srch').css('display','none');
	  	$('.no-found').css('display','none');
	  	$('.modal-pagination').html('');
	    if(tt.length>=1)
	    {
	      var pgcount = ((tt.length - 1) / perpage) + 1;
	      var cal = start * perpage;
	      var strt = cal - perpage;

	      if(pgcount>=1)
	      {
	        for(var p=1;p<=pgcount;p++)
	        {
	        	if(start==p)
	        	{
	        		$('.modal-pagination').append('<li class="active"><a href="javascript:;"  data-page="'+p+'">'+p+'</a></li>');
	        	}else{
	          		$('.modal-pagination').append('<li><a href="javascript:;" class="pagination_click" data-page="'+p+'">'+p+'</a></li>');
	        	}
	        }
	        var lmtpage = strt + perpage;

	        if(tt.length <= lmtpage)
	        {
	          lmtpage = tt.length;
	        }

	        for(var t= strt;t<lmtpage;t++)
	        {
	        	$(".srch:contains("+tt[t]+")").css("display", "block");
	        }
	      }
	    }else{
	    	$('.srch').css('display','none');
	    	$('.no-found').css('display','block');
	  		$('.modal-pagination').html('');
	    }
	  });
	}

	searchcolletion(1,'',12);

	$(function(){	

		$(document).on('click','.pagination_click',function(e){
			e.preventDefault();
			var srval = $('#search_collection').val();
			var srpg = $(this).attr('data-page');
			searchcolletion(srpg,srval,12);
		});	

		$('#search_collection').keyup(function(){
		    var srval = $(this).val();
		 	searchcolletion(1,srval,12);
		 });

		$('#add_new_cl').click(function(){
			$('.active-box').slideUp();
			$('#add_cl_div').slideDown();
			$('.collection-add-btn').css('display','none');
		});
		$('.back-btn').click(function(){
			$('#add_cl_div').slideUp();
			$('.active-box').slideDown();
			$('.collection-add-btn').css('display','block');
		});
		$('.collection-list').click( function(){
			$(this).addClass('active');
		});
		$('#add_cl_form').submit(function(){
			$('#alertdiv').css('display','none');
			$.ajax({
				type:'post',
				url:'/add-new-collection',
				data:$(this).serialize()+'&p='+'109569',
				success:function(data){
					$('#add_cl_form')[0].reset();
					$('#add_cl_div').css('display','none');
					var obj=JSON.parse(data);
					if(obj.success=='true')
					{
						window.location.reload();
					}else{
						$('#alertdiv').html('something went wrong..');
						$('#alertdiv').css('display','block');
					}
				}
			});
			// $('form button[type="submit"]').prop("disabled", true);
		});
		$(document).on('click','.add-to-cl',function(){
			$('#alertdiv').css('display','none');
			var cl_value=$(this).attr('data-id');
			if(cl_value!='')
			{
				$.ajax({
					type:'post',
					url:'/add-image-collection',
					data:'cl='+cl_value+'&p='+'109569',
					success:function(data){
						// $('#add_cl_form')[0].reset();
						// $('#add_cl_div').css('display','none');
						var obj=JSON.parse(data);
						if(obj.success=='true')
						{
							window.location.reload();
						}else{
							$('#alertdiv').html('This image is already added in this collection.');
							$('#alertdiv').css('display','block');
						}
					}
				});
			}
		});

		$(document).on('click','.delete-from-cl',function(){
			$('#alertdiv').css('display','none');
			var cl_value=$(this).attr('data-id');
			if(cl_value!='')
			{
				$.ajax({
					type:'post',
					url:'/remove-image-collection',
					data:'cl='+cl_value+'&p='+'109569',
					success:function(data){
						// $('#add_cl_form')[0].reset();
						// $('#add_cl_div').css('display','none');
						var obj=JSON.parse(data);
						if(obj.success=='true')
						{
							window.location.reload();
						}else{
							$('#alertdiv').html('something went wrong..');
							$('#alertdiv').css('display','block');
						}
					}
				});
			}
		});
	});
	$('#flexgridPhotogr1').flexImages({rowHeight: 180, truncate: 1, maxRows: 3});
</script>